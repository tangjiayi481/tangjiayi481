    pen.setpos(0, 0)
    pen.pendown()
    for i in range(30):
        pen.color("blue")
        pen.forward(200)
        pen.left(170)
    pen.penup()
    pen.setpos(0, 0)
    pen.pendown()
    for i in range(30):
        pen.color("purple")
        pen.forward(200)
        pen.left(170)
    pen.penup()
    pen.setpos(0, 0)
